mutation MyMutation($objects: [keterangan_insert_input!] = {}) {
    insert_keterangan(objects: $objects, on_conflict: {constraint: keterangan_pkey}) {
      returning {
        id
        id_anggota
        nilai
        pelajaran
        status
      }
    }
}
  


